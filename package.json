{
  "name": "gemini-proxy",
  "version": "1.0.0",
  "description": "Simple local proxy to forward requests to Google Gemini (Generative Language) with CORS support",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"node server.js\" \"vite\"",
    "dev:one": "node devstart.js",
    "build": "vite build",
    "build:production": "set NODE_ENV=production && vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "start:production": "set NODE_ENV=production && node server.js",
    "production": "npm run build:production && npm run start:production"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^6.3.0",
    "node-fetch": "^3.3.2"
  }
}
